{
  "name": "srag/dic",
  "description": "Use all ILIAS globals in your class",
  "version": "0.33.2",
  "extra": {
    "ilias_plugin": {
      "ilias_min_version": "6.0",
      "ilias_max_version": "7.999"
    }
  },
  "type": "library",
  "keywords": [
    "dic",
    "globals",
    "ilias",
    "trait"
  ],
  "homepage": "https://github.com/fluxapps/DIC",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "srag/generateplugininfoshelper": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\DIC\\OpencastObject\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "update-version": [
      "srag\\GeneratePluginInfosHelper\\UpdateVersion::updateVersion"
    ]
  }
}
